<?xml version="1.0"?>
<launch>

<!-- blue side robot -->
  <group ns="blue_bot">
      <!-- enemy bot run -->
      <include file="$(find burger_war)/launch/your_burger.launch">
        <arg name="robot_name" value="blue_bot" />
      </include>
  </group>

<!-- red side robot -->
  <group ns="red_bot">
      <!-- Your robot control node run -->
      <node pkg="burger_war" type="randomRun.py" name="randomRun" output="screen"/>
        <!--arg name="robot_name" value="blue_bot" /-->
  </group>

</launch>

